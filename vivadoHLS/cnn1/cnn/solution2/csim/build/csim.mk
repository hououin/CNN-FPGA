override TARGET := csim.exe

AUTOPILOT_ROOT := C:/Xilinx2020/Vivado/2020.1
AUTOPILOT_MACH := win64
ifdef AP_GCC_M32
  AUTOPILOT_MACH := Linux_x86
  IFLAG += -m32
endif
ifndef AP_GCC_PATH
